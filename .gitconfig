[user]
	name = mcpeapsUnterstrichHD
	email = mcpeaps_HD@outlook.com

[alias]
    push = jj git push $*
    status = jj status $*
    pull = jj pull $*
    clone = jj git clone --colocate $*

[pull]
    rebase = true

[core]
	eol = lf
	autocrlf = false
	symlinks = true

[fsck]
	zeroPaddedFilemode = ignore

[fetch "fsck"]
	zeroPaddedFilemode = ignore
[receive "fsck"]
	zeroPaddedFilemode = ignore
[oh-my-zsh]
	lastVersion = 8510847ff38a142850e4a065dc3cf09555260ff7
[remote "origin"]
	url = https://github.com/Homebrew/brew
	fetch = +refs/heads/*:refs/remotes/origin/*
[homebrew]
	analyticsmessage = true
	caskanalyticsmessage = true
[remote "upstream"]
	gh-resolved = base
