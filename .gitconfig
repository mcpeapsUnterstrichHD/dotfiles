[alias]
    push = "!jj git push $*"
    status = "!jj status $*"
    pull = "!jj pull $*"
    clone = "!jj git clone --colocate $*"


[pull]
rebase = false
[core]
	eol = lf
	autocrlf = false
[fsck]
	zeroPaddedFilemode = ignore
[fetch "fsck"]
	zeroPaddedFilemode = ignore
[receive "fsck"]
	zeroPaddedFilemode = ignore
